    "state": {
      "type": "string"
    },
    "city": {
      "type": "string"
    },
    "address": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    },
    "app": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::app.app"
    }
